{
    "name": "pdl-syntax-highlight",
    "displayName": "PDL Syntax Highlight",
    "description": "Highlight your PDL (Problem Description Language) code.",
    "version": "0.0.1",
    "publisher": "Guyutongxue",
    "repository": "https://github.com/Guyutongxue/PDL-Syntax-Highlight",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pdl",
                "aliases": [
                    "PDL",
                    "pdl"
                ],
                "extensions": [
                    ".pdl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pdl",
                "scopeName": "source.pdl",
                "path": "./syntaxes/pdl.tmLanguage.json"
            }
        ]
    }
}